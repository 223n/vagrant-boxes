{
  "variables": {
    "sakura_token": "{{env `SAKURACLOUD_ACCESS_TOKEN`}}",
    "sakura_token_secret": "{{env `SAKURACLOUD_ACCESS_TOKEN_SECRET`}}",
    "sakura_zone": "{{env `SAKURACLOUD_ZONE`}}",
    "centos_version": "7",
    "release_point": "1611",
    "sha1_checksum": "156f4cb711fdbd668943711f853fb6da89581aad"
  },
  "builders": [
    {
      "type": "sakuracloud",
      "access_token": "{{user `sakura_token`}}",
      "access_token_secret": "{{user `sakura_token_secret`}}",
      "zone": "{{user `sakura_zone`}}",
      "archive_name": "jessie",
      "os_type": "iso",
      "iso_url": "http://ftp.jaist.ac.jp/pub/Linux/CentOS/{{user `centos_version`}}/isos/x86_64/CentOS-{{user `centos_version`}}-x86_64-NetInstall-{{user `release_point`}}.iso",
      "iso_checksum": "{{user `sha1_checksum`}}",
      "iso_checksum_type": "sha1",
      "boot_wait": "5s",
      "boot_command": [
        "<tab> text ks^http<leftShiftOn>;<leftShiftOff>//wate.github.io/installer/centos/7/kickstart.cfg<enter><wait>"
      ],
      "ssh_password": "vagrant",
      "ssh_username": "vagrant"
    }
  ],
  "provisioners": [
    {
        "type": "shell",
        "inline": [
            "echo hoge"
        ]
    }
  ]
}
